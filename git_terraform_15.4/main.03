resource "aci_filter" "allow_MySQL" {
  tenant_dn = aci_tenant.tenant_tf.id
  name      = "allow_MySQL"
}

resource "aci_filter_entry" "MySQL" {
  name        = "MySQL"
  filter_dn   = aci_filter.allow_MySQL.id
  ether_t     = "ip"
  prot        = "tcp"
  d_from_port = "3306"
  d_to_port   = "3306"
  stateful    = "yes"
}
